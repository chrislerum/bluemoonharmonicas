%td
  = link_to powder_coated_cover do
    - if powder_coated_cover.powder_coated_cover_images.first
      = image_tag powder_coated_cover.powder_coated_cover_images.first.photo, width: 250
    - else
      = image_tag 'default_main_image.jpg', width: 250
    %h3
      %span.controls
        = humanized_money_with_symbol powder_coated_cover.price
      = link_to powder_coated_cover.name, powder_coated_cover
